{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx",
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    },
    "github": {
      "disabled": false,
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "autoApprove": [
        "create_entities",
        "create_relations",
        "add_observations",
        "delete_entities",
        "delete_observations",
        "delete_relations",
        "read_graph",
        "search_nodes",
        "open_nodes"
      ],
      "command": "npx"
    },
    "notionMCP": {
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.notion.com/mcp"
      ],
      "command": "npx"
    },
    "paperclip": {
      "autoApprove": [
        "tools_list_providers",
        "tools_search_papers",
        "tools_get_paper_by_id",
        "tools_get_paper_metadata_by_id",
        "tools_get_paper_content_by_url"
      ],
      "url": "https://paperclip.matsjfunke.com/mcp"
    },
    "postgres": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "${POSTGRESQL_URL}"
      ],
      "command": "npx"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx"
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false
    },
    "neovim": {
      "autoApprove": [
        "read_file",
        "read_multiple_files",
        "list_directory",
        "find_files"
      ],
      "disabled": false,
      "disabled_tools": [
        "delete_items",
        "execute_command",
        "execute_lua",
        "move_item"
      ]
    }
  }
}
